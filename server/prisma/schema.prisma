generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id       String @id @default(uuid())
  email    String @unique
  name     String?
  password String
  files    File[]
}

model File {
  id      String @id @default(uuid())
  name    String
  ownerId String
  owner   User   @relation(fields: [ownerId], references: [id])
}